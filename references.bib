@inproceedings{na2019,
  title = {Analysis of {{Multipactor Effects}} by a {{Particle-in-Cell Algorithm Coupled}} with the {{Furman-Pivi Secondary Electron Emission Model}}},
  booktitle = {2019 {{United States National Committee}} of {{URSI National Radio Science Meeting}} ({{USNC-URSI NRSM}})},
