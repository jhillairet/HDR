}

@article{colas2015,
  title = {Localized {{Scrape}}-{{Off Layer}} Density Modifications by {{Ion Cyclotron}} near Fields in {{JET}} and {{ASDEX}}-{{Upgrade L}}-Mode Plasmas},
  author = {Colas, L. and Jacquet, Ph. and Van Eester, D. and Bobkov, V. and Brix, M. and Meneses, L. and Tamain, P. and Marsen, S. and Silva, C. and Carralero, D. and Ko{\v c}an, M. and M{\"u}ller, H.-W. and Cromb{\'e}, K. and K{\v r}ivska, A. and Goniche, M. and Lerche, E. and Rimini, F.G.},
  year = {2015},
