}

@article{pfefferle2016,
  title = {Effects of Magnetic Ripple on {{3D}} Equilibrium and Alpha Particle Confinement in the {{European DEMO}}},
  author = {Pfefferl{\'e}, D. and Cooper, W.A. and Fasoli, A. and Graves, J.P.},
  year = {2016},
  month = nov,
