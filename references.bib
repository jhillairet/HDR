@article{goniche2013,
  title = {Lower Hybrid Current Drive at High Density on {{Tore Supra}}},
  author = {Goniche, M. and Basiuk, V. and Decker, J. and Sharma, P.K. and Antar, G. and {Berger-By}, G. and Clairet, F. and Delpech, L. and Ekedahl, A. and Gunn, J. and Hillairet, J. and Litaudon, X. and Mazon, D. and Nilsson, E. and Oosako, T. and Peysson, Y. and Preynas, M. and Prou, M. and S{\'e}gui, J.L.},
