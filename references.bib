@article{zhang2017-1,
  ids = {zhang2017},
  title = {{{3D}} Simulations of Gas Puff Effects on Edge Plasma and {{ICRF}} Coupling in {{JET}}},
  author = {Zhang, W. and Jacquet, P. and Lerche, E. and Bilato, R. and Bobkov, V. and Coster, D. and Feng, Y. and Guillemaut, C. and Goniche, M. and Harting, D. and Lunt, T. and Noterdaeme, J.-M. and Szepesi, G. and Van Eester, D. and {the JET Contributors}},
  year = {2017},
