@article{diez2020,
  title = {First Evidence of Optical Hot Spots on {{ITER-like}} Plasma Facing Units in the {{WEST}} Tokamak},
  author = {Diez, M. and Gunn, J. P. and Firdaouss, M. and Grosjean, A. and Corre, Y. and Delmas, E. and Gargiulo, L. and Tsitrone, E.},
