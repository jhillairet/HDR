  author = {Hoang, G.T. and B{\'e}coulet, A. and Jacquinot, J. and Artaud, J.F. and Bae, Y.S. and Beaumont, B. and Belo, J.H. and {Berger-By}, G. and Bizarro, Jo{\~a}o P.S. and Bonoli, P. and Cho, M.H. and Decker, J. and Delpech, L. and Ekedahl, A. and Garcia, J. and Giruzzi, G. and Goniche, M. and Gormezano, C. and Guilhem, D. and Hillairet, J. and Imbeaux, F. and Kazarian, F. and Kessel, C. and Kim, S.H. and Kwak, J.G. and Jeong, J.H. and Lister, J.B. and Litaudon, X. and Magne, R. and Milora, S. and Mirizzi, F. and Namkung, W. and Noterdaeme, J.M. and Park, S.I. and Parker, R. and Peysson, Y. and Rasmussen, D. and Sharma, P.K. and Schneider, M. and Synakowski, E. and Tanga, A. and Tuccillo, A. and Wan, Y.X.},
