@article{heuraux2015,
  title = {Simulation as a Tool to Improve Wave Heating in Fusion Plasmas},
  author = {Heuraux, S. and {da Silva}, F. and Ribeiro, T. and Despres, B. and Campos Pinto, M. and Jacquot, J. and Faudot, E. and Wengerowsky, S. and Colas, L. and Lu, L.},
