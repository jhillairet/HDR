  author = {Yan, Rong and Moser, Lucas and Wang, Baoguo and Peng, Jiao and Vorpahl, Christian and Leipold, Frank and Reichle, Roger and Ding, Rui and Chen, Junling and Mu, Lei and Steiner, Roland and Meyer, Ernst and Zhao, Mingzhong and Wu, Jinhua and Marot, Laurent},
