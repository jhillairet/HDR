  title = {Dispersion Relations in Cold Magnetized Plasmas},
  author = {Cheverry, Christophe and Fontaine, Adrien and {,Institut Math\'ematique de Rennes, Campus de Beaulieu, 263 avenue du G\'en\'eral Leclerc CS 74205,35042 Rennes Cedex, France}},
  year = {2017},
