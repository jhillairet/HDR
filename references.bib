@article{dinca2009,
  title = {Characterization of Arcs in {{ICRF}} Transmission Lines},
  author = {D'Inca, R. and Onyshchenko, A. and Braun, F. and Siegl, G. and Bobkov, V. and Faugel, H. and Noterdaeme, J.-M.},
