  title = {The Upgraded {{TOMAS}} Device: {{A}} Toroidal Plasma Facility for Wall Conditioning, Plasma Production, and Plasma\textendash Surface Interaction Studies},
  shorttitle = {The Upgraded {{TOMAS}} Device},
  author = {Goriaev, A. and Wauters, T. and M{\"o}ller, S. and Brakel, R. and Brezinsek, S. and Buermans, J. and Cromb{\'e}, K. and Dinklage, A. and Habrichs, R. and H{\"o}schen, D. and Krause, M. and Kovtun, Yu. and {L{\'o}pez-Rodr{\'i}guez}, D. and Louche, F. and Moon, S. and Nicolai, D. and Thomas, J. and Ragona, R. and Rubel, M. and R{\"u}ttgers, T. and Petersson, P. and Brunsell, P. and Linsmeier, Ch. and Van Schoor, M.},
