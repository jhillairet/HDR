@article{sakharov2016,
  title = {Particle-in-Cell Simulation of Multipactor Discharge on a Dielectric in a Parallel-Plate Waveguide},
  author = {Sakharov, A. S. and Ivanov, V. A. and Konyzhev, M. E.},
  year = {2016},
  month = jun,
