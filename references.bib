@article{jacquet2012,
  title = {Improvement of {{ICRF}} Antenna Loading by Local Gas Injection on {{ASDEX Upgrade}}},
  author = {Jacquet, P. and Bobkov, V. and Mayoral, M.-L. and Monakhov, I. and Noterdaeme, J.M. and Scarabosio, A. and Stepanov, I. and Vrancken, M. and Wolfrum, E. and {the ASDEX Upgrade Team}},
