@article{bae2003,
  ids = {bae2003-2},
  title = {Design of 5.0-{{GHz KSTAR}} Lower-Hybrid Coupler},
  author = {Bae, Y.S and Paek, C.H and Rhee, M.J and Namkung, W and Cho, M.H and Bernabei, S and Park, H},
