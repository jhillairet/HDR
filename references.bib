  author = {{deMingo}, J. and Valdovinos, A. and Crespo, A. and Navarro, D. and Garcia, P.},
  year = {2004},
  month = feb,
  journal = {IEEE Transactions on Microwave Theory and Techniques},
  volume = {52},
