@article{gormezano1985,
  title = {Lower-Hybrid Plasma Heating via a New Launcher \textendash{} the Multijunction Grill},
  author = {Gormezano, C. and Briand, P. and Briffod, G. and Hoang, G.T. and N'Guyen, T.K. and Moreau, D. and Ray, G.},
  year = {1985},
  month = apr,
  volume = {25},
  pages = {419--423},
  issn = {0029-5515, 1741-4326},
  doi = {10/b6r6xk},
  abstract = {A new multijunction grill-type launcher has been tested on the Petula-B tokamak. In this new launcher, the RF power is divided by means of an E-plane junction, and the phase between each resultant wave is obtained by a suitable reduction in the height of the waveguides. Data obtained on Petula-B indicate that both the heating efficiency (4.5 eV X 1013 cm"3 -kW"1) and the parametric dependences of the reflection coefficient are very similar to those of a conventional grill. Therefore, such a multyunction grill may greatly simplify the construction of grills considered for use in future large-scale tokamaks.},
