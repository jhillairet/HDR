@article{ding2017,
  title = {Effort of Lower Hybrid Current Drive Experiments toward to {{H-mode}} in {{EAST}}},
  author = {Ding, B.J. and Li, M.H. and Liu, F.K. and Shan, J.F. and Li, Y.C. and Wang, M. and Liu, L. and Zhao, L.M. and Yang, Y. and Wu, Z.G. and Feng, J.Q. and Hu, H.C. and Jia, H. and Cheng, M. and Zang, Q. and Lyu, B. and Duan, Y.M. and Lin, S.Y. and Wu, J.H. and Hillairet, J. and Ekedahl, A. and Peysson, Y. and Goniche, M. and Tuccillo, A.A. and Cesario, R. and Amicucci, L. and Shen, B. and Gong, X.Z. and Xu, G.S. and Zhao, H.L. and Hu, L.Q. and Li, J.G. and Wan, B.N. and {EAST Team}},
