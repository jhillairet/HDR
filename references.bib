  author = {Murphy, N. A. and Sta{\'n}czak, D. and Kozlowski, P. M. and Leonard, A. J. and Langendorf, S. J. and Beckers, J. P. and Mumford, S. and Malhotra, R. and Parashar, T. and Haggerty, C. C. and Huang, Y. M.},
  year = {2018},
  month = dec,
  journal = {AGU Fall Meeting Abstracts},
  volume = {11},
  abstract = {PlasmaPy is a new openly developed community Python package that strives to provide the core functionality needed by heliospheric, laboratory, and astrophysical plasma physicists. Because of the interdisciplinary nature of plasma physics, PlasmaPy is being developed to be compatible with Astropy, SunPy, and related packages in the emerging Python ecosystem for heliophysics. Current functionality includes the calculation of plasma parameters and transport coefficients, methods for accessing atomic properties, mathematical functions commonly used by plasma physicists, and a prototype abstract base class to describe arbitrary plasma data sets. After providing plans for future development, we will discuss the lessons we have learned through starting an open source project for scientific community, including the importance of having a code of conduct and fostering a welcoming community.},
