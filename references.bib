@article{guilhem2011,
  title = {Manufacturing Process and Tests of a {{Lower Hybrid Passive}}\textendash{{Active Multijunction}} Launcher for Long Pulse Experiments on {{Tore-Supra}}},
  author = {Guilhem, D. and Samaille, F. and Bertrand, B. and Lipa, M. and Achard, J. and Agarici, G. and Argouarch, A. and Armitano, A. and Belo, J.H. and Bej, Z. and {Berger-By}, G. and Bouquey, F. and Brun, C. and Chantant, M. and {E.Corbel} and Delmas, E. and Delpech, L. and Doceul, L. and Ekedahl, A. and Faisse, F. and Fejoz, P. and Goletto, C. and Goniche, M. and Hatchressian, J.C. and Hillairet, J. and Houry, M. and Joanard, J.P. and Joubert, P. and Lambert, R. and Lombard, G. and Lyonne, M. and Madeleine, S. and Magne, R. and Marfisi, L. and Martinez, A. and Maury, M. and Missirlian, M. and Mollard, P. and Poli, S. and Portafaix, C. and Preynas, M. and Prou, M. and Raulin, D. and Rousset, E. and Saille, A. and Soler, B. and Thouvenin, D. and Verger, J.M. and Volpe, D. and Vulliez, K. and Zago, B.},
  year = {2011},
  month = oct,
  journal = {Fusion Engineering and Design},
  volume = {86},
  number = {6-8},
  pages = {513--517},
  issn = {09203796},
  doi = {10/dmtckt},
  abstract = {A new concept of multijunction-type antenna has been developed, the Passive\textendash Active Multijunction, which improves the cooling of the waveguides and the damping of the neutron energy (for ITER) compared to Full Active Multijunction. Due to the complexity of the structures, prototypes of the mode converters and of the Passive\textendash Active-Multijunction launcher were fabricated and tested, in order to validate the different manufacturing processes and the manufacturer's capability to face this challenging project. This paper describes the manufacturing process, the tests of the various prototypes and the construction of the final Passive\textendash Active-Multijunction launcher, which entered into operation in October 2009. It has been commissioned and is fully operational on the Tore-Supra tokamak, since design objectives were reached in March 2010: 2.75 MW \textendash{} 78 s, power density of 25 MW/m2 in active waveguides, steady-state apparent surface temperatures {$<$}350 {$\smwhtcircle$}C; 10 cm long distance coupling.},
  langid = {english},
  keywords = {\#nosource}
}

@article{guilhem2014,
  title = {Implications of the {{TORE-SUPRA WEST-Project}} on {{Radio Frequency Additional Heating Systems}}},
  author = {Guilhem, Dominique and Argouarch, Arnaud and Bernard, Jean-Michel and Bouquey, Francis and Colas, Laurent and Delpech, Lena and Durodie, Frederic and Ekedahl, Annika and {van Helvoirt}, Jan and Hillairet, Julien and Joffrin, Emmanuel and Litaudon, Xavier and Magne, Roland and Milanesio, Daniele and Moerel, Jovita Gerardus Maria and Mollard, Patrick and Wittebol, Erik Henribus Maria and Achard, Joelle and Armitano, Arthur and {Berger-By}, Gilles and Charabot, Nicolas and Goniche, Marc and Jacquot, Jonathan and Lombard, Gilles and Prou, Marc and {Traisnel-Corbel}, Elodie and Volpe, Robert and Vulliez, Karl},
  year = {2014},
  month = mar,
  journal = {IEEE Transactions on Plasma Science},
  volume = {42},
  number = {3},
  pages = {879--884},
  issn = {0093-3813, 1939-9375},
  doi = {10/ggc263},
  abstract = {This year, TORE-SUPRA celebrated 25 years of operation. During this time, a number of technologies have been developed. First of all, it was mandatory to develop reliable superconducting magnets at {$\sim$}1.8 K, with superfluid helium as an efficient coolant. For the production of steady-state discharge, three types of radio-frequency (RF) additional heating systems have been developed: lower hybrid current drive, ions and electrons cyclotron resonance heating. To cope with long-lasting discharges (up to 380 s\texttimes 2.8 MW) and large RF additional heating power (12.3 MW\texttimes 3 s), actively cooled (AC) plasma facing components were deployed in TORE-SUPRA for the first time in a tokamak environment. TORE-SUPRA is now being modified into a D-shaped axisymmetric tokamak with AC main chamber walls and an AC tungsten divertor, the W\textemdash for tungsten\textemdash Environment in Steady-state tokamak (WEST). This new facility has the objective to offer ITER a test bed for validating the relevant AC metallic technologies in D-shaped H-mode plasmas. In contrast to other metallic devices such as JET and ASDEX upgrade, WEST will rely only on RF additional power systems. A set of plasma scenarios have been identified, ranging from a high total RF power scenario up to 15 MW\textendash 30 s, to a high fluence scenario of 1000 s with up to 10 MW of injected RF power. These scenarios are able to reproduce ITER-relevant conditions of steady-state heat loads of 10\textendash 20 MW/m2, to test tungsten AC divertor technologies with relevant power heat fluxes and particle fluence.},
  langid = {english},
  keywords = {\#nosource}
}

