@article{argouarch2009,
  title = {{{ICRH ITER-like}} Antenna Tested on {{TS}} Commissioning, Electrical Modeling and Load Resilience Studies},
  author = {Argouarch, A. and Vulliez, K. and Mollard, P. and Lombard, G. and Bosia, G. and Colas, L. and Mendes, A. and Volpe, D. and Millon, L. and Ekedahl, A. and Bremond, S. and Clairet, F. and Hoang, G.T. and Mendes, A. and Elkhaldi, M. and Beaumont, B. and B{\'e}coulet, A. and {Berger-by}, G. and Giruzzi, G. and Gunn, J. and Hertout, P. and Rimini, F. and {Saint-Laurent}, F.},
