  doi = {10/fhvwnv},
  abstract = {The effect on coupling of an evanescent layer in front of ion cyclotron fast-wave antennas is commonly described by the exponential factor exp(-2k d), with k the parallel wave vector of the peak of the launched wave spectrum and d the width of the evanescence layer. With an elementary one-dimensional model, we demonstrate that this dependence is too pessimistic, as confirmed by recent experiments.},
  langid = {english}
}

