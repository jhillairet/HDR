  shorttitle = {Toward {{Industrial Exploitation}} of {{THz Frequencies}}},
  author = {Campion, James and Hassona, Ahmed and He, Zhongxia Simon and Beuerle, Bernhard and {Gomez-Torrent}, Adrian and Shah, Umer and Vecchiattini, Sandro and Lindman, Richard and Dahl, Torbj{\"o}rn S. and Li, Yinggang and Zirath, Herbert and Oberhammer, Joachim},
  year = {2019},
