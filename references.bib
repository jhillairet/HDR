@inproceedings{ravera2012,
  title = {Analysis of the {{FAST ICRH}} Antenna with Salted Water Load Using {{HFSS}} Code},
  booktitle = {2012 42nd {{European Microwave Conference}}},
  author = {Ravera, Gian Luca and Ceccuzzi, Silvio and Mirizzi, Francesco and Tuccillo, Angelo A. and Maggiora, Riccardo},
  year = {2012},
