@inproceedings{kishek2014,
  title = {A {{Global Approach}} for {{Analyzing Multipactor}}},
  booktitle = {Proceedings of the 8th {{International Workshop}} on {{Multipactor}}, {{Corona}}, and {{Passive Intermodulation}}, {{Valencia}}, {{Spain}} ({{MULCOPIM}})},
  author = {Kishek, Rami A},
  year = {2014},
  pages = {6},
  address = {{Valencia (Spain)}},
  abstract = {Multipactor is a pernicious electrical discharge that can damage or destroy microwave components and related electronics, such as antennas, satellite payloads, or radar systems. Driven by the emission of secondary electrons from the surface of the structure, multipactor can result in rapid growth in the number of electrons. Past theoretical studies of multipactor have relied on two approaches: (a) Analytical treatment of the simplest cases, e.g., a resonant mode in a parallel plate structure; (b) Numerical simulation involving integration of trajectories of many particles (tracking), or Monte Carlo models. Simulations have demonstrated considerable success in predicting multipactor for specific devices, but the results are difficult to generalize and the scalings are not obvious. Detailed analytical treatment of higher-periodicity modes becomes overwhelmingly complex, especially with the addition of surface charging or magnetic fields. This paper presents a novel approach, based on systematic application of iterative maps, capable of analyzing a wide range of multipactor problems. The new methodology borrows the standard tools of nonlinear dynamics and chaos to identify multipacting region boundaries and stability more reliably and comprehensively than existing models. It does so by globally analyzing the structure of dynamical space rather than individually analyzing every possible multipacting mode.},
  langid = {english},
  keywords = {\#nosource,⛔ No DOI found}
}

