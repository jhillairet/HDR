  author = {Saito, K. and Wang, S.J. and Wi, H.H. and Kim, H.J. and Kamio, S. and Nomura, G. and Seki, R. and Seki, T. and Kasahara, H. and Mutoh, T.},
  year = {2019},
  month = sep,
  journal = {Fusion Engineering and Design},
  volume = {146},
  pages = {256--260},
  issn = {09203796},
  doi = {10/ggc29q},
  abstract = {In the Large Helical Device (LHD), the development of high-power and long-pulse Ion Cyclotron Range of Frequencies (ICRF) heating system is ongoing. The developed Field-Aligned-Impedance-Transforming (FAIT) antenna has the potential for high-power injection of more than 1.8 MW. Here, to achieve this injection power, a power combination system was developed. An optimized power combiner was designed by repeated simulations, and then was fabricated and installed in the ICRF transmission system. Control of the power and the phase of incident waves into the input ports of the power combiner is important for the power combination. Therefore, a real-time control system was developed, and prompt reduction of power loss was demonstrated. As a result, combined powers of more than 2 MW for 6 s and 1 MW for 10 min were successfully achieved.},
  langid = {english},
  keywords = {\#nosource}
}

