@article{humphries2017,
  title = {Noise {{Radar Approach}} for {{Interrogating SAW Sensors Using Software Defined Radio}}},
  author = {Humphries, James R. and Reed, Frederick K. and Carmichael, Christopher P. and Fuhr, Peter L. and McIntyre, Timothy J. and Weeks, Arthur R. and Malocha, Donald C.},
  year = {2017},
  month = oct,
  journal = {IEEE Sensors Journal},
  volume = {17},
  number = {20},
  pages = {6760--6769},
  issn = {1558-1748},
  doi = {10/gcf249},
  abstract = {Passive, wireless surface acoustic wave (SAW) sensor systems can be approached from a radar perspective, where the SAW device is thought of as a cooperative target. This paper investigates the use of a commercial-off-the-shelf software defined radio to interrogate wireless SAW sensors with a randomly generated interrogation pulse. The USRP B200mini is utilized as the transceiver platform with custom field-programmable gate array (FPGA) modifications to generate the random interrogation waveform and provide synchronization and buffering to the received signal. Each transmit sample bit in the FPGA is fed by an independent linear-feedback shift register, which generates pseudo-random I and Q samples for the interrogation pulse. An RF daughterboard has also been developed and integrated with the B200mini to increase the transmit power, provide filtering of the RF signals, and switch a signal antenna between the transmit and receive channels. Radio control and matched filter correlator post-processing are accomplished using Python. Design and implementation details for the FPGA modifications, RF daughterboard, and post-processing are discussed. The system is demonstrated by wirelessly interrogating SAW temperature sensors at 915 MHz.},
  keywords = {Bandwidth,field programmable gate array,field programmable gate arrays,FPGA,frequency 915 MHz,matched filter correlator postprocessing,noise radar,passive SAW sensor,Python,Radar,Radar antennas,radar signal processing,radio control,radiotelemetry,random interrogation waveform,randomly generated interrogation pulse,SAW sensor,SAW sensor interrogation,SAW temperature sensors,SDR,Sensor systems,Signal to noise ratio,software defined radio,software radio,surface acoustic wave sensors,Surface acoustic waves,temperature sensors,USRP B200mini transceiver,waveform generators,wireless surface acoustic wave sensor}
