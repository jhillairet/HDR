@article{katsav2009,
  title = {Gaussian {{Beams Summation Representation}} of {{Half Plane Diffraction}}: {{A Full}} 3-{{D Formulation}}},
  shorttitle = {Gaussian {{Beams Summation Representation}} of {{Half Plane Diffraction}}},
  author = {Katsav, Michael and Heyman, Ehud},
