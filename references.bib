@article{balcon2012,
  title = {Secondary {{Electron Emission}} on {{Space Materials}}: {{Evaluation}} of the {{Total Secondary Electron Yield From Surface Potential Measurements}}},
  shorttitle = {Secondary {{Electron Emission}} on {{Space Materials}}},
  author = {Balcon, N. and Payan, D. and Belhaj, M. and Tondu, T. and Inguimbert, V.},
