@article{stevens1988,
  title = {Lower Hybrid Experiments on {{PLT}} Using Grills with Various n {\textsubscript{{$\parallel$}}} Spectral Widths},
  author = {Stevens, J.E. and Bell, R.E. and Bernabei, S. and Cavallo, A. and Chu, T.K. and Colestock, P.L. and Hooke, W. and Hosea, J. and Jobes, F. and Luce, T. and Mazzucato, E. and Motley, R. and Pinsker, R. and Von Goeler, S. and Wilson, J.R.},
  year = {1988},
  month = feb,
  journal = {Nuclear Fusion},
  volume = {28},
  number = {2},
  pages = {217--230},
  issn = {0029-5515, 1741-4326},
  doi = {10.1088/0029-5515/28/2/004},
  abstract = {Coupling structures for lower hybrid current drive experiments have, until now, been smaller than a free space wavelength and have had a correspondingly broad wavenumber spectrum. The paper reports the results of experiments on the PLT tokamak using a 16-waveguide grill (2.2 wavelengths) which produces a very narrow n| = k|C/w spectrum. Experimental results from the 16-waveguide grill are compared with results from three other PLT grills with less sharply defined n| spectra. The current drive figure of merit, Ip\^R/P,\^ = 0.14 x 10l4 (A-cm\textasciitilde 3-m/W), is approximately 40\% higher for the experiments with the 16-waveguide coupler than for previously reported experiments on PLT, in spite of the larger 'spectral gap'. The experimental results are consistent with the first-pass damping of a large fraction of the launched spectrum.},
  langid = {english},
  keywords = {\#nosource}
}

@article{stix1957,
  title = {Oscillations of a {{Cylindrical Plasma}}},
  author = {Stix, Thomas H.},
  year = {1957},
  month = jun,
  journal = {Physical Review},
  volume = {106},
  number = {6},
  pages = {1146--1150},
  publisher = {{American Physical Society}},
  doi = {10/bgg5bs},
  abstract = {The natural modes of oscillation of a cylindrical plasma of finite density at zero pressure in a longitudinal magnetic field are examined. When one considers frequencies well below the electron plasma and electron cyclotron frequencies, there appear in two limiting cases hydromagnetic waves, and waves whose natural frequencies are close to the ion cyclotron frequency. This cyclotron frequency resonance occurs when the wavelength of the oscillation in the z direction is short compared to the wavelength for light in vacuo corresponding to the root square sum of the ion plasma frequency and the ion cyclotron frequency.}
}

