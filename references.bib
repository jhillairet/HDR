  title = {Plasma Cleaning of Beryllium Coated Mirrors},
  author = {Moser, L and Marot, L and Steiner, R and Newman, M and Widdowson, A and Ivanova, D and Likonen, J and Petersson, P and Pintsuk, G and Rubel, M and Meyer, E and Contributors, Jet},
