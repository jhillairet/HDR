@article{fujii1992,
  title = {Performance of the {{JT-60 ICRF}} Antenna with an Open Type {{Faraday}} Shield},
  author = {Fujii, T and Saigusa, M and Kimura, H and Moriyama, S and Annoh, K and Kawano, Y and Kobayashi, N and Kubo, H and Nishitani, T and Ogawa, Y and Shinozaki, S and Terakado, M},
