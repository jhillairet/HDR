@inproceedings{pauer2002,
  title = {Creepage of {{Copper}} in {{Mechanically}} and {{Thermally Highly-Stressed Plug Connectors}}},
  booktitle = {Proc. of 21st {{ICEC}}},
  author = {Pauer, Ralph and L{\"o}bl, Helmut},
  year = {2002},
  pages = {6},
  abstract = {Plug connections usually consist of contacts with built-in spring elements which provide the contact force. When there is a requirement for the transmission of large currents at high current densities copper is used for the current path. Under high mechanical stress copper tends to creep depending on the stress, duration and temperature. This means, that the contact force decreases with time and thus, the contact resistance increases. As soon as the contact force drops below a certain minimum level the connection looses its reliability especially at higher current values. This ageing process is determined by the creep of copper. The process is calculated using a finite element model. For the calculation it is necessary to experimentally determine the relevant creep parameters. Using a contact element as an example it is shown how large the deformation of the copper in the highly stressed regions is and how large the resulting loss in contact force is. The comparison between calculation and measurement is in good agreement.},
  langid = {english},
  keywords = {\#nosource,⛔ No DOI found}
}

@article{paul2007,
  title = {Wave-Induced Helicity Current Drive by Helicon Waves},
  author = {Paul, Manash Kr and Bora, Dhiraj},
  year = {2007},
