@article{messiaen1986,
  title = {Ion Cyclotron Resonance Heating on {{TEXTOR}}},
  author = {Messiaen, A. M. and Bhatnagar, V. P. and Delvigne, T. and Descamps, P. and Durodie, F. and Jadoul, M. and Koch, R. and Lebeau, D. and Pearson, D. I. C. and Vandenplas, P. E. and Vanderstraeten, A. and Nieuwenhove, R. Van and Oost, G. Van and Wassenhove, G. Van and Weynants, R. R. and Bay, H. L. and Bertschinger, G. and Bieger, W. and Bogen, P. and Campbell, G. A. and Conn, R. W. and Dippel, K. H. and Esser, H. G. and Finken, K. H. and Fuchs, G. and Giesen, B. and Goebel, D. M. and Graffmann, R. and Guthrie, S. E. and Hartwig, H. and Hintz, E. and Hrehuss, G. and Hoenen, F. and Hoethker, K. and Kaleck, A. and Konen, L. and Korten, M. and Lie, Y. T. and Liung, K. and Lochter, M. and Pontau, A. E. and Pospieszczyk, A. and Rusbuldt, D. and Samm, U. and Schweer, B. and Schluter, J. and Soltwisch, H. and Thomas, G. and Waelbroeck, F. and Waidmann, G. and Wienhold, P. and Winter, J. and Wolf, G. H.},
  year = {1986},
  month = jan,
  journal = {Plasma Physics and Controlled Fusion},
  volume = {28},
  number = {1A},
  pages = {71--83},
  publisher = {{IOP Publishing}},
  issn = {0741-3335},
  doi = {10/dg8wjf},
  abstract = {Ion cyclotron heating on TEXTOR has now reached the Megajoule level. The heating scenario is normally mode conversion but occasionally minority heating in a D-(H) plasma. With appropriate wall conditioning by carbonization more than 1 MW of RF power has been injected for long pulse durations ( approximately 1 s). The ICRF heated plasma is characterized by a quasi-stationarity of all plasma parameters, little if no impurity increase and a loop voltage reduction resulting in the total power coupled to the plasma reaching six times the remaining ohmic power input. Evidence of the coupling of the RF power to the plasma is obtained from the increase of the thermal load on the limiters and central energy deposition is supported from analysis of the sawtooth heating rate.},
  langid = {english}
}

@article{messiaen1989,
