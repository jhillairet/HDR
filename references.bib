  author = {Anza, S. and Vicente, C. and Gil, J. and Boria, V. E. and Gimeno, B. and Raboso, D.},
  year = {2010},
  month = jun,
  journal = {Physics of Plasmas},
  volume = {17},
  number = {6},
  pages = {062110},
  publisher = {{American Institute of Physics}},
  issn = {1070-664X},
  doi = {10/ffsrr3},
  abstract = {This work presents a new and general approach to the real dynamics of the multipactor process: the nonstationary statistical multipactor theory. The nonstationary theory removes the stationarity assumption of the classical theory and, as a consequence, it is able to adequately model electron exponential growth as well as absorption processes, above and below the multipactor breakdown level. In addition, it considers both double-surface and single-surface interactions constituting a full framework for nonresonant polyphase multipactor analysis. This work formulates the new theory and validates it with numerical and experimental results with excellent agreement.}
}

