@inproceedings{yastrebov2015,
  title = {Three-Level Multi-Scale Modeling of Electrical Contacts Sensitivity Study and Experimental Validation},
  booktitle = {2015 {{IEEE}} 61st {{Holm Conference}} on {{Electrical Contacts}} ({{Holm}})},
  author = {Yastrebov, Vladislav A. and Cailletaud, Georges and Proudhon, Henry and Mballa, Frederick S. Mballa and Noel, Sophie and Teste, Philippe and Houze, Frederic},
  year = {2015},
  month = oct,
