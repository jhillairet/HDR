@article{zhao2014,
  title = {{{EAST}} Ion Cyclotron Resonance Heating System for Long Pulse Operation},
  author = {Zhao, Y.P. and Zhang, X.J. and Mao, Y.Z. and Yuan, S. and Xue, D.Y. and Deng, X. and Wang, L. and Ju, S.Q. and Cheng, Y. and Qin, C.M. and Chen, G. and Lin, Y. and Li, J.G. and Wan, B.N. and Song, Y.T. and Braun, F. and Kumazawa, R. and Wukitch, S.},
