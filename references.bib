@article{maebara2000,
  title = {Outgassing of Plasma Facing Antenna Front for Lower Hybrid Wave Launcher},
  author = {Maebara, Sunao and Goniche, Marc and Kazarian, Fabienne and Seki, Masami and Ikeda, Yoshitaka and Imai, Tsuyoshi and Bibet, Philippe and Froissard, Philippe and Rey, Guy},
  year = {2000},
