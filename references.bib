@article{wallace2011,
  title = {Lower Hybrid Current Drive at High Density in {{Alcator C-Mod}}},
  author = {Wallace, G.M. and Hubbard, A.E. and Bonoli, P.T. and Faust, I.C. and Harvey, R.W. and Hughes, J.W. and LaBombard, B.L. and Meneghini, O. and Parker, R.R. and Schmidt, A.E. and Shiraiwa, S. and Smirnov, A.P. and Whyte, D.G. and Wilson, J.R. and Wright, J.C. and Wukitch, S.J. and {the Alcator C-Mod Team}},
  year = {2011},
  month = aug,
  journal = {Nuclear Fusion},
  volume = {51},
  number = {8},
  pages = {083032},
  issn = {0029-5515, 1741-4326},
  doi = {10.1088/0029-5515/51/8/083032},
  abstract = {Experimental observations of lower hybrid current drive (LHCD) at high density on the Alcator C-Mod tokamak are presented in this paper. Bremsstrahlung emission from relativistic fast electrons in the core plasma drops suddenly above line averaged densities of 1020 m-3 ({$\omega$}/{$\omega$}LH {$\sim$} 3) in single null discharges with large ({$>$} 10 mm) plasma\textendash inner wall gaps, well below the density limit previously observed on limited tokamaks ({$\omega$}/{$\omega$}LH {$\sim$} 2). Modeling and experimental evidence suggest that the absence of LHCD driven fast electrons at high density may be due to parasitic collisional absorption in the scrape off layer. Experiments show that the population of fast electrons produced by LHCD at high density (n\textasciimacron e {$>$} m-3) can be increased significantly by operating with a plasma\textendash inner wall gap of less than {$\sim$} 5 mm with the strongest non-thermal emission in inner-wall limited plasmas. A change in plasma topology from single to double null produces a modest increase in non-thermal emission at high density. Increasing the electron temperature in the periphery of the plasma (0.8 {$>$} r/a {$>$} 1.0) also results in a modest increase in non-thermal electron emission above the density limit.},
  langid = {english},
  keywords = {\#nosource}
}

@article{wallace2013,
  title = {Advances in Lower Hybrid Current Drive Technology on {{Alcator C-Mod}}},
  author = {Wallace, G.M. and Shiraiwa, S. and Hillairet, J. and Preynas, M. and Beck, W. and Casey, J.A. and Doody, J. and Faust, I.C. and Fitzgerald, E. and Johnson, D.K. and Kanojia, A.D. and Koert, P. and Lau, C. and Lin, Y. and Leccacorvi, R. and MacGibbon, P. and Meneghini, O. and Murray, R. and Parker, R.R. and Terry, D.R. and Vieira, R. and Wilson, J.R. and Wukitch, S. and Zhou, L.},
  year = {2013},
  month = jul,
  journal = {Nuclear Fusion},
  volume = {53},
  number = {7},
  pages = {073012},
  issn = {0029-5515, 1741-4326},
  doi = {10/ggc2x8},
  abstract = {Lower hybrid current drive (LHCD) is an attractive option for non-inductive tokamak operation due to its high current drive efficiency and ability to drive current off axis. The parameters of the Alcator C-Mod LHCD system (f0 = 4.6 GHz, B{$\varphi$} 5.5 T, n\textasciimacron e 1020 m-3) are similar to the proposed LHCD system on ITER. This paper will describe improvements in LHCD technology on C-Mod designed to increase single-pass absorption at high n\textasciimacron e, extend pulse length (to {$>$}3 s), and increase power delivered to the plasma (to {$\sim$}2 MW). Modelling of lower hybrid (LH) wave propagation indicates that the observed loss of LHCD efficiency at higher n\textasciimacron e can be mitigated by enhancing the single pass power absorption through use of an off mid-plane launcher. The four rows of the launcher are located above the mid-plane (with Ip and B{$\varphi$} both clockwise viewing from the top down) in order to exploit the poloidal upshift of n as rays propagate from the antenna into the plasma. The transmitter protection system (TPS) was redesigned to model the coolant temperature in real time and shut off the klystron beam voltage if the coolant is close to boiling. The TPS upgrade has been installed and operated on C-Mod for pulses up to 4.5 s into dummy loads and 1.0 s into the plasma. A new movable local LH launcher protection limiter was designed to reduce reflection coefficients across a wide range of launcher positions. Finally, a high power waveguide double-stub tuner is under development to provide feedback controlled load matching to reduce power reflected from the antenna under poor coupling conditions.},
  langid = {english},
  keywords = {\#nosource}
}

