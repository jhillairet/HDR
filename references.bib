@article{rasch2010,
  title = {Microwave {{Multipactor Breakdown Between Two Cylinders}}},
  author = {Rasch, J. and Anderson, D. and Johansson, J. F. and Lisak, M. and Puech, J. and Rakova, E. and Semenov, V. E.},
  year = {2010},
  month = aug,
  journal = {IEEE Transactions on Plasma Science},
  volume = {38},
  number = {8},
  pages = {1997--2005},
  issn = {1939-9375},
  doi = {10/d5p257},
  abstract = {An analysis has been made of the microwave breakdown threshold for multipactor in an open structure comprising two parallel cylinders, approximating, e.g., parts of a helix antenna. The electron motion in the corresponding electromagnetic field is analyzed by separating the motion into a slowly varying drift velocity (driven by the ponderomotive force due to the electric field inhomogeneity) and a rapidly oscillating part (driven by the oscillating electric field). Furthermore, the curvature of the cylindrical surfaces of emission is shown to give rise to a new effect that implies a loss of electrons. This leads to a more stringent multipactor breakdown condition for the two-wire structure than for the classical situation corresponding to the case of two plane parallel infinite plates. The importance of this effect is determined by the ratio of the cylinder radii and the distance between the cylinders, and it is shown that when this ratio is small, multipactor can only occur for surfaces having very large secondary emission coefficients. A detailed analysis is also made to determine the lowest voltage between the cylinders at which multipactor becomes possible.},
  keywords = {cylinder radii ratio,drift velocity,electric breakdown,Electric breakdown,Electromagnetic analysis,electromagnetic field,Electromagnetic fields,Electron emission,electron loss,Electron mobility,electron motion,Helical antennas,helix antenna,Helix antenna,Microwave antennas,microwave breakdown,microwave multipactor breakdown threshold,microwave switches,Motion analysis,multipactor,Nonuniform electric fields,parallel cylinders,secondary emission coefficients,two plane parallel infinite plates,two-wire structure,Voltage}
}

@article{rasch2011,
  title = {Simulations of {{Multipactor Breakdown Between Two Cylinders}}},
  author = {Rasch, J. and Semenov, V. E. and Rakova, E. and Anderson, D. and Johansson, J. F. and Lisak, M. and Puech, J.},
  year = {2011},
  month = sep,
  journal = {IEEE Transactions on Plasma Science},
  volume = {39},
  number = {9},
  pages = {1786--1794},
  issn = {1939-9375},
  doi = {10/cx93s3},
  abstract = {Simulations have been performed to determine the multipactor breakdown threshold in a microwave structure composed of two parallel cylinders, chosen to be an approximate model of an open helix microwave antenna system. The electromagnetic field between the cylinders is available in closed analytical form, and a Monte Carlo software has been developed to calculate the 2-D electron trajectories and to simulate the multipactor avalanche in this inhomogeneous electric field for different ratios of cylinder radius and distance of separation between the cylinders. The results are compared with those of a recently published analytical theory and show a qualitatively good agreement. In particular, it is confirmed that, for a given distance between cylinders, there exists a smallest cylinder radius below which no two-sided multipactor breakdown can occur. The basic physical explanation is a loss mechanism for secondary emitted electrons that is caused by the curvature of the cylinder surfaces together with the strong electric field at the surfaces. The results imply that the breakdown threshold in realistic open helix antennas is significantly higher than those predicted using extrapolations based on resonance theory and the classical two parallel plate model.},
  keywords = {Approximation methods,Dispersion,Electric breakdown,Geometry,Helical antennas,Helix antenna,microwave breakdown,multipactor,Trajectory,Wires}
}

