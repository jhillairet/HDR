@article{rantamaki2000,
  title = {Estimation of Heat Loads on the Wall Structures in Parasitic Absorption of Lower Hybrid Power},
  author = {Rantam{\"a}ki, K.M and P{\"a}ttikangas, T.J.H and Karttunen, S.J and Bibet, P and Litaudon, X and Moreau, D},
