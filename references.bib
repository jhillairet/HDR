  author = {Bobkov, V. and Braun, F. and Dux, R. and Herrmann, A. and Faugel, H. and F{\"u}nfgelder, H. and Kallenbach, A. and Neu, R. and Noterdaeme, J.-M. and Ochoukov, R. and P{\"u}tterich, Th. and Tuccilo, A. and Tudisco, O. and Wang, Y. and Yang, Q. and {ASDEX Upgrade team}},
  year = {2016},
