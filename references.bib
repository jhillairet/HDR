@article{yajima2019,
  title = {Development of Capacitively-Coupled Combline Antennas for Current Drive in Tokamaks},
  author = {Yajima, S. and Takase, Y. and Tajiri, Y. and Takei, Y. and Tsujii, N. and Yamazaki, H. and Moeller, C.P. and Shinya, T.},
