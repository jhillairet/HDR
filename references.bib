@article{zhang2016-1,
  title = {Modelling of the {{ICRF}} Induced {{{\emph{E}}}} \texttimes{} {{{\emph{B}}}} Convection in the Scrape-off-Layer of {{ASDEX Upgrade}}},
  author = {Zhang, W and Feng, Y and Noterdaeme, J-M and Bobkov, V and Colas, L and Coster, D and Lunt, T and Bilato, R and Jacquot, J and Ochoukov, R and Van Eester, D and K{\v r}ivsk{\'a}, A and Jacquet, P and Guimarais, L and {the ASDEX Upgrade Team}},
