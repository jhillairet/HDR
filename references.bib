  author = {Decker, J. and Peysson, Y. and Hillairet, J. and Artaud, J.-F. and Basiuk, V. and Becoulet, A. and Ekedahl, A. and Goniche, M. and Hoang, G.T. and Imbeaux, F. and Ram, A.K. and Schneider, M.},
  year = {2011},
