@article{chen2016-1,
  title = {Study on the {{L}}\textendash{{H}} Transition Power Threshold with {{RF}} Heating and Lithium-Wall Coating on {{EAST}}},
  author = {Chen, L. and Xu, G. S. and Nielsen, A. H. and Gao, W. and Duan, Y. M. and Liu, H. Q. and Wang, L. and Li, M. H. and Wang, M. and Zhang, X. J. and Chen, R. and Wang, H. Q. and Sun, Z. and Ding, S. Y. and Yan, N. and Liu, S. C. and Shao, L. M. and Zhang, W. and Hu, G. H. and Li, J. and Zhang, L. and {and}, B. N. Wan},
  year = {2016},
  month = apr,
  volume = {56},
  pages = {056013},
  issn = {0029-5515},
  doi = {10/ggnctz},
  abstract = {The power threshold for low (L) to high (H) confinement mode transition achieved by radio-frequency (RF) heating and lithium-wall coating is investigated experimentally on EAST for two sets of walls: an all carbon wall (C) and molybdenum chamber and a carbon divertor (Mo/C). For both sets of walls, a minimum power threshold P thr of 0.6 MW was found when the EAST operates in a double null (DN) divertor configuration with intensive lithium-wall coating. When operating in upper single null (USN) or lower single null (LSN), the power threshold depends on the ion {$\nabla$}B drift direction. The low density dependence of the L\textendash H power threshold, namely an increase below a minimum density, was identified in the Mo/C wall for the first time. For the C wall only the single-step L\textendash H transition with limited injection power is observed whereas also the so-called dithering L\textendash H transition is observed in the Mo/C wall. The dithering behaves distinctively in a USN, DN and LSN configuration, suggesting the divertor pumping capability is an important ingredient in this transition since the internal cryopump is located underneath the lower divertor. Depending on the chosen divertor configuration, the power across the separatrix P loss increases with neutral density near the lower X-point in EAST with the Mo/C wall, consistent with previous results in the C wall (Xu et al 2011 Nucl. Fusion 51 072001). These findings suggest that the edge neutral density, the ion {$\nabla$}B drift as well as the divertor pumping capability play important roles in the L\textendash H power threshold and transition behaviour.},
  file = {C\:\\Users\\JH218595\\Zotero\\storage\\V2UBQGYY\\Chen et al_2016_Study on the L–H transition power threshold with RF heating and lithium-wall.pdf},
  journal = {Nuclear Fusion},
  language = {en},
  number = {5}
}

@article{chen2016-2,
  ids = {chen2016-3},
  title = {Development and Test of Decoupler for {{ICRF}} Antenna in {{EAST}}},
  author = {Chen, Gen and Mao, Yuzhou and Zhao, Yanping and Yuan, Shuai and Zhang, Xinjun and Qing, Chengming},
  year = {2016},
  month = jun,
  volume = {107},
