  author = {Mullen, J. H. and Davis, J. W.},
  year = {1984},
  month = jun,
  journal = {IEEE Transactions on Plasma Science},
  volume = {12},
  number = {2},
  pages = {79--95},
  issn = {1939-9375},
  doi = {10/cx296x},
  abstract = {In order to have successful and reliable high-power ion cyclotron resonance heating (ICRH) launchers operating in 5-MW power range by 1986-1988 time range, much research and development work is required. A discussion of the major electrical and thermodynamic problems associated with development of these high-power launchers is presented and two candidate high-power launchers are described in detail. One, an integrally tunable cavity-backed U-slot, can be flush mounted, can operate at high power, and can be tuned over a 2:1 frequency band. The other, a dielectrically loaded U-slot, can also be flush mounted, also operates at high power, but can operate at lower frequencies. Required tests and test scenarios for both hot (with plasma) and cold (no plasma) facilities are discussed. In addition, a brief discussion of potential reactor relevant ICRH materials is also included.},
  keywords = {Cyclotrons,Dielectrics,Frequency,Heating,Inductors,Plasma materials processing,Research and development,Resonance,Testing,Thermodynamics}
}

