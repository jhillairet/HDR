  author = {Ceccuzzi, S. and Meschino, S. and Mirizzi, F. and Artaud, J.F. and Bae, Y.S. and Belo, J. and {Berger-By}, G. and Bernard, J.M. and Cara, Ph. and Cardinali, A. and Castaldo, C. and Cesario, R. and Decker, J. and Delpech, L. and Ekedahl, A. and {J.Garcia} and Garibaldi, P. and Goniche, M. and Guilhem, D. and Hillairet, J. and Hoang, G.T. and Huang, Q.Y. and Imbeaux, F. and Jia, H. and Kim, S.H. and Lausenaz, Y. and Maggiora, R. and Magne, R. and Marfisi, L. and Milanesio, D. and Namkung, W. and Pajewski, L. and Panaccione, L. and Peysson, Y. and Saille, A. and Schettini, G. and Schneider, M. and Sharma, P.K. and Tuccillo, A.A. and Tudisco, O. and Vecchi, G. and Villari, R. and Vulliez, K. and Wu, Y. and Zeng, Q.},
