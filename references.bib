@article{monakhov2005,
  ids = {monakhov2005-1},
  title = {Tests of Load-Tolerant External Conjugate-{{T}} Matching System for {{A2 ICRF}} Antenna at {{JET}}},
  author = {Monakhov, I. and Walden, A. and Blackman, T. and Child, D. and Graham, M. and Hardiman, W. and Lamalle, P. U. and Mayoral, M. -L. and Nightingale, M. and Whitehurst, A.},
