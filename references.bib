  abstract = {High harmonic fast wave (HHFW) current drive at frequency near the lower hybrid wave is calculated under CFETR conditions. Modelling results show that the HHFW has excellent off-axis current drive capacity with high efficiency in a wide range of n// (1.5\textendash 2.9) and wave frequency (0.6\textendash 2.4 GHz), the peak driven current is about 35 kA/MW and the current drive efficiency {$\eta$} = neICDR/P {$\sim$} 2.7\texttimes 1019 AW-1 m-2. Under the same parameters, smaller n// leads to more peaked driven current and higher wave frequency corresponding to more outside current peak position. The comparisons of driven current properties with LHCD indicate that the current drive efficiency of HHFW is comparable to LHCD and the driven current profile is also more flexible. Scanning results of density and temperature show that low density and/or high temperature help to obtain more driven current, and the current drive efficiency can be improved at high temperatures and/or high densities.},
  file = {C\:\\Users\\JH218595\\Zotero\\storage\\D7KJ3W59\\Zhu et al_2019_Investigation of high harmonic fast wave for current drive on CFETR.pdf;C\:\\Users\\JH218595\\Zotero\\storage\\NUGVC4NR\\S0920379619306817.html},
  journal = {Fusion Engineering and Design},
  keywords = {CFETR,Current drive,Efficiency,HHFW},
  language = {en}
}

@article{zinkle2014,
  title = {Evaluation of High Strength, High Conductivity {{CuNiBe}} Alloys for Fusion Energy Applications},
  author = {Zinkle, S.J.},
  year = {2014},
  month = jun,
  volume = {449},
  pages = {277--289},
  issn = {00223115},
  doi = {10/f5566t},
  abstract = {The unirradiated tensile properties for several different heats and thermomechanical treatment conditions of precipitation strengthened Hycon 3HP\texttrademark{} CuNiBe (Cu\textendash 2\%Ni\textendash 0.35\%Be in wt.\%) have been measured over the temperature range of 20\textendash 500 \textdegree C for longitudinal and long transverse orientations. The room temperature electrical conductivity has also been measured for several heats, and the precipitate microstructure was characterized using transmission electron microscopy. The CuNiBe alloys exhibit very good combination of strength and conductivity at room temperature, with yield strengths of 630\textendash 725 MPa and electrical conductivities of 65\textendash 72\% International Annealed Copper Standard (IACS). The strength remained relatively high at all test temperatures, with yield strengths of 420\textendash 520 MPa at 500 \textdegree C. However, low levels of ductility ({$<$}5\% uniform elongation) were observed at test temperatures above 200\textendash 250 \textdegree C, due to flow localization near grain boundaries (exacerbated by having only 10\textendash 20 grains across the gage thickness of the miniaturized sheet tensile specimens). Scanning electron microscopy observation of the fracture surfaces found a transition from ductile transgranular to ductile intergranular fracture with increasing test temperature. Fission neutron irradiation to a dose of \$0.7 displacements per atom (dpa) at temperatures between 100 and 240 \textdegree C produced a slight increase in strength and a significant decrease in ductility. The measured tensile elongation after irradiation increased with increasing irradiation temperature, with a uniform elongation of \$3.3\% observed at 240 \textdegree C. The electrical conductivity decreased slightly following irradiation, due to the presence of defect clusters and Ni, Zn, Co transmutation products. Considering also previously published fracture toughness data, this indicates that CuNiBe alloys have irradiated tensile and electrical properties comparable or superior to CuCrZr and oxide dispersion strengthened copper at temperatures {$<$}250 \textdegree C, and may be an attractive candidate for certain lowtemperature fusion energy structural applications. Conversely, CuNiBe may not be preferred at intermediate temperatures of 250\textendash 500 \textdegree C due to the poor ductility and fracture toughness of CuNiBe alloys at temperatures P250 \textdegree C. The potential deformation mechanisms responsible for the transition from transgranular to intergranular fracture are discussed. The possible implications for other precipitation-hardened alloys such as nickel based superalloys are briefly discussed.},
  file = {C\:\\Users\\JH218595\\Documents\\Bibliography\\Papers\\Materials and Co\\Zinkle_2014_Evaluation of high strength, high conductivity CuNiBe alloys for fusion energy applications.pdf},
  journal = {Journal of Nuclear Materials},
  language = {en},
  number = {1-3}
}

