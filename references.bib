@article{chou2003,
  title = {Application of {{Gaussian}}-Ray Basis Functions for the Rapid Analysis of Electromagnetic Radiation from Reflector Antennas},
  author = {Chou, H.-T. and Pathak, P.H. and Burkholder, R.J.},
  year = {2003},
