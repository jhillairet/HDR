  author = {Milanesio, D. and Hillairet, J. and Panaccione, L. and Maggiora, R. and Artaud, J. F. and Bae, Y. S. and Barbera, A. M. A. and Belo, J. and {Berger-By}, G. and Bernard, J. M. and Cara, Ph. and Cardinali, A. and Castaldo, C. and Ceccuzzi, S. and Cesario, R. and Decker, J. and Delpech, L. and Ekedahl, A. and Garcia, J. and Garibaldi, P. and Goniche, M. and Guilhem, D. and {Hamlyn-Harris}, C. and Hoang, G. T. and Jia, H. and Huang, Q. Y. and Imbeaux, F. and Kim, S. H. and Lausenaz, Y. and Magne, R. and Marfisi, L. and Meschino, S. and Mirizzi, F. and Namkung, W. and Pajewski, L. and Peysson, Y. and Saille, A. and Schettini, G. and Schneider, M. and Sharma, P. K. and Tuccillo, A. A. and Tudisco, O. and Villari, R. and Vulliez, K. and Wu, Y. and Zeng, Q.},
  year = {2011},
  month = oct,
