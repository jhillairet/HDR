  author = {Ganji, Jayesh and Jain, Yogesh M. and Parmar, Pramod R. and Ambulkar, Kirankumar and Sharma, P. K. and Dixit, Harish V.},
  year = {2022},
  month = feb,
  journal = {Fusion Engineering and Design},
  volume = {175},
  pages = {113000},
  issn = {0920-3796},
  doi = {10/gn3htp},
  abstract = {Fast Wave Current Drive (FWCD) has the potential to drive non-inductive current in high beta plasmas. A novel interdigital type Travelling Wave Antenna (TWA) was proposed by the authors to launch fast waves inside SST-1 plasma at an operating frequency of 500~MHz and a parallel refractive index of 4.0. A prototype interdigital TWA has been fabricated and tested for its RF response at low power. The simulated and measured frequency responses with vacuum loading are in excellent agreement and the TWA exhibits band-pass like characteristics with a return loss of {$>$} 25~dB and insertion loss of 0.45~dB to 1.0 dB. Measurement studies have also been carried out with saltwater loading for various gaps between antenna mouth and saltwater edge and with different conductivity in water. Simulated and measured results are found to be in good agreement with saltwater loading as well. The n{$\parallel$} spectrum is also measured and it has been observed that n{$\parallel$} spectrum peaks at a value of 4.0.},
  langid = {english},
  keywords = {Fabrication,FWCD,Prototype Interdigital TWA,spectrum,SST-1}
}

