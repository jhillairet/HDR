@article{mantsinen2004,
  title = {Localized Bulk Electron Heating with {{ICRF}} Mode Conversion in the {{JET}} Tokamak},
  author = {Mantsinen, M.J and Mayoral, M.-L and Eester, D. Van and Alper, B and Barnsley, R and Beaumont, P and Bucalossi, J and Coffey, I and Conroy, S and de Baar, M. and de Vries, P. and Erents, K and Figueiredo, A and Gondhalekar, A and Gowers, C and Hellsten, T and Joffrin, E and Kiptily, V and Lamalle, P.U and Lawson, K and Lyssoivan, A and Mailloux, J and Mantica, P and Meo, F and Milani, F and Monakhov, I and Murari, A and Nguyen, F and Noterdaeme, J.-M and Ongena, J and Petrov, Yu and Rachlew, E and Riccardo, V and Righi, E and Rimini, F and Stamp, M and Tuccillo, A.A and Zastrow, K.-D and Zerbini, M and {contributors}, Jet Efda},
  year = {2004},
  month = jan,
  journal = {Nuclear Fusion},
  volume = {44},
  number = {1},
  pages = {33--46},
  issn = {0029-5515, 1741-4326},
  doi = {10/bjb7gj},
  abstract = {Ion cyclotron resonance frequencies (ICRF) mode conversion has been developed for localized on-axis and off-axis bulk electron heating on the JET tokamak. The fast magnetosonic waves launched from the low-field side ICRF antennas are mode-converted to short-wavelength waves on the high-field side of the 3He ion cyclotron resonance layer in D and 4He plasmas and subsequently damped on the bulk electrons. The resulting electron power deposition, measured using ICRF power modulation, is narrow with a typical full-width at half-maximum of {$\approx$}30 cm (i.e. about 30\% of the minor radius) and the total deposited power to electrons comprises at least up to 80\% of the applied ICRF power. The ICRF mode conversion power deposition has been kept constant using 3He bleed throughout the ICRF phase with a typical duration of 4\textendash 6 s, i.e. 15\textendash 40 energy confinement times. Using waves propagating in the counter-current direction minimizes competing ion damping in the presence of co-injected deuterium beam ions.},
  langid = {english}
}

