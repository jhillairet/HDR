  author = {Durodi{\'e}, F. and Vrancken, M. and Bamber, R. and Colas, L. and Dumortier, P. and Hancock, D. and Huygen, S. and Lockley, D. and Louche, F. and Maggiora, R. and Milanesio, D. and Messiaen, A. and Nightingale, M. P. S. and Shannon, M. and Tigwell, P. and Van Schoor, M. and Wilson, D. and Winkler, K. and {CYCLE Team}},
  year = {2014},
