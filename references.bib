@article{gandini2001,
  title = {The Detection of the Non-Absorbed Millimeterwave Power during {{EC}} Heating and Current Drive},
  author = {Gandini, F and Cirant, S and Hirsch, M and Laqua, H.P and Nowak, S and Bruschi, A and Granucci, G and Erckmann, V and Mellera, V and Muzzini, V and Nardone, A and Simonetto, A and Sozzi, C and Spinicchia, N},
  year = {2001},
  month = oct,
  journal = {Fusion Engineering and Design},
  volume = {56--57},
  pages = {975--979},
  issn = {09203796},
  doi = {10/d2k8fw},
  abstract = {Fusion-oriented devices using powerful source of electron cyclotron (EC) waves are subjected to high levels of RF-stray radiation. This radiation is potentially harmful for any object made of RF-absorbing material exposed to it. Special material or/and suitable techniques to scrambling the radiation could be needed, depending on the effective expected level of such radiation. Therefore arises the need for a reliable method to foresee the distribution and the level of the RF-stray radiation. A multi-modal antenna, with sensitivity approximately constant for any incoming linear polarization, with great radiating surface and low directivity has been developed. Its purpose is collecting 140 GHz stray radiation from FTU tokamak and Wendelstein 7-AS stellarator plasmas during ECRH and current drive experiments. Theoretical predictions for stray radiation levels are obtained by two different methods: a multiple ray tracing code developed for FTU and a multi-cell code, originally written for W7-X. Both codes have been applied to FTU geometry, resulting in a good agreement with the experimental data from the multi-modal antenna. Descriptions of the antenna design and of the results from both codes are presented. \textcopyright{} 2001 Elsevier Science B.V. All rights reserved.},
  langid = {english},
  keywords = {\#nosource}
}

@article{gandini2011,
  title = {The {{EC H}}\&{{CD Transmission Line}} for {{ITER}}},
  author = {Gandini, F. and Bigelow, T. S. and Becket, B. and Caughman, J. B. and Cox, D. and Darbos, C. and Gassmann, T. and Henderson, M. A. and Jean, O. and Kajiwara, K. and Kobayashi, N. and Nazare, C. and Oda, Y. and Omori, T. and Purohit, D. and Rasmussen, D. A. and Ronden, D. M. S. and Saibene, G. and Sakamoto, K. and Shapiro, M. A. and Takahashi, K. and Temkin, R. J.},
