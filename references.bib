@article{darbos2009,
  title = {The 118-{{GHz Electron Cyclotron Heating System}} on {{Tore Supra}}},
  author = {Darbos, C. and Magne, R. and Arnold, A. and Prinz, H. O. and Thumm, M. and Bouquey, F. and Hogge, J. P. and Lambert, R. and Lennholm, M. and Li{\'e}vin, C. and Traisnel, E.},
  year = {2009},
  month = oct,
  volume = {56},
