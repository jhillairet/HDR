@article{murphy2019-1,
  title = {{{PlasmaPy}}: An Open Source Package for Plasma Physics},
  shorttitle = {{{PlasmaPy}}},
  author = {Murphy, Nicholas A. and Sta{\'n}czak, Dominik and Kozlowski, Pawel M. and Malhotra, Ritiek and Langendorf, Samuel J. and Leonard, Andrew J. and Stansby, David and Mumford, Stuart J. and Beckers, Jasper P. and Parashar, Tulasi N. and Everson, Erik and Schaffner, David and Community, PlasmaPy},
  year = {2019},
  month = may,
  pages = {79},
  abstract = {Plasma physics is essential to heliophysics.  The emerging Python ecosystem for heliophysics must therefore include plasma physics functionality.  PlasmaPy is an open source Python package for plasma physics that is being developed to provide the core functionality needed to investigate heliospheric, laboratory, and astrophysical plasmas. PlasmaPy is being developed to be compatible with Astropy and SunPy. PlasmaPy's second development release includes the calculation of plasma parameters and transport coefficients, methods for accessing atomic data, and the prototype of a class to describe arbitrary plasmas.  We will describe PlasmaPy's development roadmap, including plasma simulation capabilities, diagnostic analysis tools, and the plan for base data structures.  We will discuss the role that PlasmaPy will fill within the Python in Heliophysics Community.},
