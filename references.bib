@inproceedings{hillairet2012-1,
  title = {Lower {{Hybrid}} Antennas for Nuclear Fusion Experiments},
  booktitle = {2012 6th {{European Conference}} on {{Antennas}} and {{Propagation}} ({{EUCAP}})},
  author = {Hillairet, J. and Achard, J. and Bae, Y. S. and Bai, X. and Balorin, C. and Baranov, Y. and Basiuk, V. and B{\'e}coulet, A. and Belo, J. and {Berger-By}, G. and Br{\'e}mond, S. and Castaldo, C. and Ceccuzzi, S. and Cesario, R. and Corbel, E. and Courtois, X. and Decker, J. and Delmas, E. and Delpech, L. and Ding, X. and Douai, D. and Ekedahl, A. and Goletto, C. and Goniche, M. and Guilhem, D. and Gunn, J. P. and Hertout, P. and Hoang, G. T. and Imbeaux, F. and Kirov, K. K. and Litaudon, X. and Magne, R. and Mailloux, J. and Mazon, D. and Mirizzi, F. and Mollard, P. and Moreau, P. and Oosako, T. and Petrzilka, V. and Peysson, Y. and Poli, S. and Preynas, M. and Prou, M. and {Saint-Laurent}, F. and Samaille, F. and Saoutic, B. and Sharma, P. K.},
  year = {2012},
  month = mar,
  pages = {1--4},
  issn = {2164-3342},
  doi = {10/ggc24b},
  abstract = {The nuclear fusion research goal is to demonstrate the feasibility of fusion power for peaceful purposes. In order to achieve the conditions similar to those expected in an electricity-generating fusion power plant, plasmas with a temperature of several hundreds of millions of degrees must be generated and sustained for long periods. For this purpose, RF antennas delivering multi-megawatts of power to magnetized confined plasma are commonly used in experimental tokamaks. In the gigahertz range of frequencies, high power phased arrays known as ``Lower Hybrid'' (LH) antennas are used to extend the plasma duration. This paper reviews some of the technological aspects of the LH antennas used in the Tore Supra tokamak and presents the current design of a proposed 20 MW LH system for the international experiment ITER.},
  copyright = {All rights reserved},
  file = {C\:\\Users\\JH218595\\Documents\\Bibliography\\Papers\\Fusion\\RF\\LHCD\\Hillairet et al._2012_Lower Hybrid antennas for nuclear fusion experiments.pdf;C\:\\Users\\JH218595\\Zotero\\storage\\34RQHY73\\6205828.html},
  keywords = {antennas in plasma,Current Drive,electricity-generating fusion power plant,fusion power feasibility,fusion reactor design,gigahertz frequency range,high power,high power phased arrays,ITER,Lower Hybrid,lower hybrid antennas,magnetized confined plasma,multimegawatt power,nuclear fusion,Nuclear Fusion,phased array,plasma temperature,plasma toroidal confinement,plasma transport processes,power 20 MW,rectangular waveguide,RF antennas,Tokamak devices,Tore Supra,Tore Supra tokamak}
}

@article{hillairet2013,
  title = {Recent Progress on Lower Hybrid Current Drive and Implications for {{ITER}}},
  author = {Hillairet, J. and Ekedahl, A. and Goniche, M. and Bae, Y.S. and Achard, J. and Armitano, A. and Beckett, B. and Belo, J. and {Berger-By}, G. and Bernard, J.M. and Corbel, E. and Delpech, L. and Decker, J. and Dumont, R. and Guilhem, D. and Hoang, G.T. and Kazarian, F. and Kim, H.J. and Litaudon, X. and Magne, R. and Marfisi, L. and Mollard, P. and Namkung, W. and Nilsson, E. and Park, S. and Peysson, Y. and Preynas, M. and Sharma, P.K. and Prou, M. and {the Tore Supra Team}},
  year = {2013},
  volume = {53},
  pages = {073004},
  issn = {0029-5515, 1741-4326},
  doi = {10/f22s7q},
  abstract = {The sustainment of steady-state plasmas in tokamaks requires efficient current drive systems. Lower Hybrid Current Drive (LHCD) is currently the most efficient method to generate a continuous additional off-axis toroidal plasma current as well as reduce the poloidal flux consumption during the plasma current ramp-up phase. The operation of the Tore Supra ITER-like LH launcher and 700kW CW klystrons have demonstrated the ability to couple LH power at ITER-like power densities with very low reflected power during long pulses. However, in order to achieve pure stationary LH sustained plasmas, some R\&D are needed to increase the reliability of all the systems and codes, from the RF sources to the plasma scenario prediction. The CEA/IRFM is addressing some of these issues by leading a R\&D program toward an ITER LH system and by the validation of an integrated LH modeling suite of codes. In 2011, the RF design of a mode converter has been validated at low power. A 500 kW/5 s RF window is currently under manufacturing and will be tested at high power in 2012 in collaboration with NFRI. All of this work aims to reduce the operational risks associated with the ITER steady-state operations.},
  copyright = {All rights reserved},
  file = {C\:\\Users\\JH218595\\Documents\\Bibliography\\Papers\\Fusion\\RF\\LHCD\\Hillairet et al._2013_Recent progress on lower hybrid current drive and implications for ITER.pdf},
  journal = {Nuclear Fusion},
  language = {en},
  number = {7}
}

