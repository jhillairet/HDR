}

@article{episkopou2012,
  title = {Defining {{Material Parameters}} in {{Commercial EM Solvers}} for {{Arbitrary Metal-Based THz Structures}}},
  author = {Episkopou, Elpida and Papantonis, Stergios and Otter, William J. and Lucyszyn, Stepan},
  year = {2012},
  month = sep,
  journal = {IEEE Transactions on Terahertz Science and Technology},
  volume = {2},
  number = {5},
  pages = {513--524},
  issn = {2156-3446},
  doi = {10/f4w35q},
  abstract = {Frequency-domain solvers are used extensively for modeling arbitrary metal-based terahertz structures. Four well-known commercially available electromagnetic (EM) modeling software packages include HFSS\texttrademark, CST Microwave Studio\textregistered, EMPro, and RSoft. However, there are a number of operational issues that relate to how they can be used to obtain more meaningful and accurate results. Even experienced users of these and similar software packages may not fully appreciate some of the subtle ambiguities in defining boundaries and material parameters for use in THz applications. To this end, a detailed comparative study has been undertaken, in consultation with all four vendors. First, in order to avoid introducing ambiguities, frequency dispersion in materials has to be clearly defined from first principles; in both intrinsic and effective forms. Different frequency dispersion models are then introduced for `metal-like' materials. To act as benchmark structures, conventional air-filled metal-pipe rectangular waveguides, associated cavity resonators and a spoof surface plasmon waveguide have been simulated, using a raft of different approaches; with a view to illustrating quantifiable weaknesses in commercial software packages for simulating arbitrary metal-based THz structures. This paper highlights intuitive and logical approaches that give incorrect results and, where possible, makes recommendations for the most appropriate solutions that have hitherto not been given in Technical Notes.},
  keywords = {benchmark structure,cavity resonator,Cavity resonators,commercial EM solvers,commercial software,Conductivity,CST Microwave Studio package,Dispersion,electromagnetic modeling software package,EMPro package,frequency dispersion,frequency domain,frequency domain solvers,frequency-domain analysis,HFSS package,Impedance,material parameter,Materials,Mathematical model,metal,metal based THz structure,Metals,numerical modeling,operational issues,resonators,RSoft package,spoof surface plasmon waveguide,Surface impedance,surface plasmons,terahertz (THz),terahertz materials,waveguides}
}

