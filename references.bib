  author = {Salza, Giuseppe and Goussetis, George and Vicente, Carlos and Kosmopoulos, Savvas and Reglero, Marta and Taroncher, M{\'a}riam and Boria, Vicente E. and Sidiropoulos, Nikolas and Galdeano, Jaione},
  year = {2019},
