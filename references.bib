  author = {Louche, F. and K{\v r}ivsk{\'a}, A. and Messiaen, A. and Ongena, J. and Borsuk, V. and Durodi{\'e}, F. and Schweer, B.},
  year = {2015},
  month = oct,
  volume = {96-97},
  pages = {508--511},
  issn = {09203796},
  doi = {10.1016/j.fusengdes.2015.01.039},
  abstract = {Ion Cyclotron Resonance Heating (ICRH) is a promising heating and wall conditioning method considered for the W7-X stellarator and a dedicated ICRH antenna has been designed. This antenna must perform several tasks in a long term physics programme: fast particles generation, heating at high densities, current drive and ICRH physics studies. Various minority heating scenarios are considered and two frequency bands will be used. In the present work a design for the low frequency range (25\textendash 38 MHz) only is developed. The antenna is made of 2 straps with tap feeds and tuning capacitors with DC capacitance in the range 15\textendash 200 pF. These capacitors introduce additional constraints on the optimisation and on the maximum amount of power that can be coupled to the plasma: not only the capacitor voltages cannot exceed a certain value (42 kV) but also the currents are limited to approximately 740 A rms to ensure sufficient heat dissipation for the considered duty cycle. Starting from an initial geometry we used the tridimensional electromagnetic software CST MicroWave Studio (MWS) to assess and optimise its coupling properties. By modifying some geometrical parameters of the front face (strap width, antenna box depth, strap length, strap feeders shape), we show that a substantial increase in maximum coupled power can be obtained accounting for the technical constraints on the capacitors. The various steps of the optimisation are validated with TOPICA simulations. For a given density profile the RF power coupling expectancy can be precisely computed.},
  file = {C\:\\Users\\JH218595\\Documents\\Bibliography\\Papers\\Fusion\\RF\\ICRH\\Louche et al._2015_Three-dimensional modelling and numerical optimisation of the W7-X ICRH antenna.pdf},
  journal = {Fusion Engineering and Design},
  language = {en}
}

@article{louche2017,
  title = {Simulation of Cold Magnetized Plasmas with the {{3D}} Electromagnetic Software {{CST Microwave Studio}} {\textsuperscript{\textregistered}}},
  author = {Louche, Fabrice and K{\v r}ivsk{\'a}, Alena and Messiaen, Andr{\'e} and Wauters, Tom},
  editor = {Hillairet, J.},
  year = {2017},
  volume = {157},
  pages = {03031},
  issn = {2100-014X},
  doi = {10/ggc28j},
  abstract = {Detailed designs of ICRF antennas were made possible by the development of sophisticated commercial 3D codes like CST Microwave Studio R (MWS). This program allows for very detailed geometries of the radiating structures, but was only considering simple materials like equivalent isotropic dielectrics to simulate the reflection and the refraction of RF waves at the vacuum/plasma interface. The code was nevertheless used intensively, notably for computing the coupling properties of the ITER ICRF antenna. Until recently it was not possible to simulate gyrotropic medias like magnetized plasmas, but recent improvements have allowed programming any material described by a general dielectric or/and diamagnetic tensor. A Visual Basic macro was developed to exploit this feature and was tested for the specific case of a monochromatic plane wave propagating longitudinally with respect to the magnetic field direction. For specific cases the exact solution can be expressed in 1D as the sum of two circularly polarized waves connected by a reflection coefficient that can be analytically computed. Solutions for stratified media can also be derived. This allows for a direct comparison with MWS results. The agreement is excellent but accurate simulations for realistic geometries require large memory resources that could significantly restrict the possibility of simulating cold plasmas to small-scale machines.},
  file = {C\:\\Users\\JH218595\\Documents\\Bibliography\\Papers\\RF - Electromagnetism\\Louche et al._2017_Simulation of cold magnetized plasmas with the 3D electromagnetic software CST Microwave Studio ®.pdf},
  journal = {EPJ Web of Conferences},
  language = {en}
}

