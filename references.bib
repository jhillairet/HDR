@article{suttle2021,
  title = {Collective Optical {{Thomson}} Scattering  in Pulsed-Power Driven High Energy Density Physics Experiments (Invited)},
  author = {Suttle, L. G. and Hare, J. D. and Halliday, J. W. D. and Merlini, S. and Russell, D. R. and Tubman, E. R. and {Valenzuela-Villaseca}, V. and Rozmus, W. and Bruulsema, C. and Lebedev, S. V.},
  year = {2021},
  month = mar,
  journal = {Review of Scientific Instruments},
  volume = {92},
  number = {3},
  pages = {033542},
  publisher = {{American Institute of Physics}},
  issn = {0034-6748},
  doi = {10/gjh945},
  abstract = {Optical collective Thomson scattering (TS) is used to diagnose magnetized high energy density physics experiments at the Magpie pulsed-power generator at Imperial College London. The system uses an amplified pulse from the second harmonic of a Nd:YAG laser (3~J, 8~ns, 532~nm) to probe a wide diversity of high-temperature plasma objects, with densities in the range of 1017\textendash 1019~cm-3 and temperatures between 10~eV and a few keV. The scattered light is collected from 100 {$\mu$}m-scale volumes within the plasmas, which are imaged onto optical fiber arrays. Multiple collection systems observe these volumes from different directions, providing simultaneous probing with different scattering K-vectors (and different associated {$\alpha$}-parameters, typically in the range of 0.5\textendash 3), allowing independent measurements of separate velocity components of the bulk plasma flow. The fiber arrays are coupled to an imaging spectrometer with a gated intensified charge coupled device. The spectrometer is configured to view the ion-acoustic waves of the collective Thomson scattered spectrum. Fits to the spectra with the theoretical spectral density function S(K, {$\omega$}) yield measurements of the local plasma temperatures and velocities. Fitting is constrained by independent measurements of the electron density from laser interferometry and the corresponding spectra for different scattering vectors. This TS diagnostic has been successfully implemented on a wide range of experiments, revealing temperature and flow velocity transitions across magnetized shocks, inside rotating plasma jets and imploding wire arrays, as well as providing direct measurements of drift velocities inside a magnetic reconnection current sheet.}
}

