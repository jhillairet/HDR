  author = {Mao, Yuzhou and Yuan, Shuai and Zhao, Yanping and Zhang, Xinjun and Chen, Gen and Cheng, Yan and Wang, Lei and Ju, Songqing and Deng, Xu and Qin, Chengming and Yang, Lei and Kumazawa, R.},
