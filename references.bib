@article{belo2011,
  title = {An {{ITER}}-Relevant Passive Active Multijunction Launcher for Lower Hybrid Current Drive in {{JET}}-Grade Plasmas},
  author = {Belo, Jorge H. and Bibet, Philippe and Bizarro, Jo{\~a}o P.S. and Bertrand, Bernard and Cesario, Roberto and Corona, Mauro and Finburg, Paul and Kaye, Alan S. and Leguern, Frederic and Lioure, Alain and Mailloux, Joelle and Mirizzi, Francesco and Ongena, Jef and Portafaix, Christophe and Testoni, Pietro and Tuccillo, Angelo A. and Vallet, Jean-Claude and Walton, Bob and Warren, Robert J. and Wilson, David J.},
