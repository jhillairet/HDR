@article{fedorczak2020,
  title = {Infra-Red Thermography Estimate of Deposited Heat Load Dynamics on the Lower Tungsten Divertor of {{WEST}}},
  author = {Fedorczak, N. and Gaspar, J. and Firdaouss, M. and Moncada, V. and Grosjean, A. and Dejarnac, R. and Brezinsek, S. and Tsitrone, E. and Bucalossi, J. and {and}, T. Loarer},
