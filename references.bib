@inproceedings{campion2018,
  title = {An {{Ultra Low-Loss Silicon-Micromachined Waveguide Filter}} for {{D-Band Telecommunication Applications}}},
  booktitle = {2018 {{IEEE}}/{{MTT-S International Microwave Symposium}} - {{IMS}}},
  author = {Campion, James and Glubokov, Oleksandr and Gomez, Adr{\'i}an and Krivovitca, Aleksandr and Shah, Umer and Bolander, Lars and Li, Yinggang and Oberhammer, Joachim},
  year = {2018},
