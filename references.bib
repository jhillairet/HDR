  pages = {1349--1362},
  issn = {07413335},
  doi = {10/dvq5qj},
  abstract = {The coupling of lower hybrid waves to the plasma is a crucial issue for efficient current drive in tokamaks. In this work, the coupling problem is attacked with a new tool in this context: an electromagnetic particle-in-cell (PIC) code, XOOPIC (Verboncoeur J P et al 1995 Comp. Phys. Comm. 87 199). A model for a grill with 32 waveguides is constructed using perfectly conducting walls. The wave propagation in the waveguides and the coupling to plasma is followed. The wave\textendash plasma interaction is studied and the evolution of the launched spectrum is resolved in the near-field of the grill. The reflection coefficients in the individual waveguides are determined.},
  langid = {english},
  keywords = {\#nosource}
}

@article{rantamaki2000,
  title = {Estimation of Heat Loads on the Wall Structures in Parasitic Absorption of Lower Hybrid Power},
  author = {Rantam{\"a}ki, K.M and P{\"a}ttikangas, T.J.H and Karttunen, S.J and Bibet, P and Litaudon, X and Moreau, D},
  year = {2000},
  month = aug,
  journal = {Nuclear Fusion},
  volume = {40},
  number = {8},
  pages = {1477--1490},
  issn = {0029-5515},
  doi = {10/c82dsg},
  abstract = {Parasitic absorption of the short wavelength modes of the LH spectrum is a probable reason for the hot spots seen in the grill region of several tokamaks. Experiments suggest that the heat loads on the wall structures depend on the coupled power. In this work, the parasitic absorption of LH power was studied with self-consistent particle-in-cell simulations. The launched spectra were obtained from the SWAN coupling code. The power and temperature dependences of the absorption in the near field of the LH grill were investigated with a series of simulations. The parasitic absorption was found to grow from 0.6 to 1.1\% when the coupled power increased from 26 to 67 MW/m2. When the edge temperature rose from 12.5 to 100 eV, the absorption increased from 0.4 to 1.7\%. The maximum kinetic energies were between 0.6 and 1.8 keV. Estimates for the heat loads and surface temperature of the grill limiter are also obtained. The absorption leads to heat loads between 1.5 and 13 MW/m2 and surface temperatures of 510\textendash 2390{$\smwhtcircle$}C.},
  langid = {english},
  keywords = {\#nosource}
}

@article{rantamaki2005,
  title = {Bright Spots Generated by Lower Hybrid Waves on {{JET}}},
  author = {Rantam{\"a}ki, K M and Petr{\v z}{\'i}lka, V and Andrew, P and Coffey, I and Ekedahl, A and Erents, K and Fuchs, V and Goniche, M and Granucci, G and Joffrin, E and Karttunen, S J and Lomas, P and Mailloux, J and Mantsinen, M and Mayoral, M-L and McDonald, D C and Noterdaeme, J-M and Parail, V and Tuccillo, A A and {\v Z}{\'a}{\v c}ek, F and Workprogramme, Contributors to the EFDA{\textemdash}JET},
