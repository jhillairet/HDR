  doi = {10/crjnbg},
  abstract = {Odd-parity rotating magnetic fields (RMFo) applied to mirror-configuration plasmas have produced average electron energies exceeding 200 eV at line-averaged electron densities of {$\sim$}1012 cm-3. These plasmas, sustained for over 103{$\tau$}Alfven, have low Coulomb collisionality, v{${_\ast}$}c{$\equiv$}L/{$\lambda$}C{$\sim$}10-3, where {$\lambda$}C is the Coulomb scattering mean free path and L is the plasma's characteristic half length. Divertors allow reduction of the electron-neutral collision frequency to values where the RMFo coupling indicates full penetration of the RMFo to the major axis.}
}

