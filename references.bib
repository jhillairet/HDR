@article{chen2020,
  title = {Surfactant-Assisted Electrodeposition of {{Au}}\textendash{{Co}}/{{WS2}} Self-Lubricating Coating from {{WS2}} Suspended Cyanide Electrolyte},
  author = {Chen, Zhaoxi and Wagner, Julien and Turq, Viviane and Hillairet, Julien and Taberna, Pierre-Louis and Laloo, Raphael and Duluard, Sandrine and Bernard, Jean-Michel and Song, Yuntao and Yang, Qingxi and Lu, Kun and Cheng, Yong},
  year = {2020},
  month = jul,
  journal = {Journal of Alloys and Compounds},
  volume = {829},
  pages = {154585},
  issn = {0925-8388},
  doi = {10/ggnh5z},
  abstract = {In this study, Triton X-100 was used as the WS2 dispersion agent in the Au\textendash Co cyanide electrolyte to deposit Au\textendash Co/WS2 composite coatings. Probe sonication was applied to exfoliate the commercial WS2 powders to produce thinner and smaller WS2 flakes, which improved the stability of the WS2 particles in the electrolyte. According to the electrochemical analyses, the effects of adding Triton X-100 and WS2 particles to the electroplating process were investigated. Through material characterizations, WS2 particles were proved to be compounded into the Au\textendash Co matrix and showed clearly \{002\} preferred orientation due to their flake structures. Tribological tests were performed under dry condition in 10-3~Pa vacuum against stainless steel 316L balls with diameters of 3~mm and a normal contact force of 2~N. The Au\textendash Co/WS2 composite coatings that developed showed the minimum coefficient of friction and wear rate of 0.05 and 8~\texttimes ~10-6~mm3/N{$\cdot$}m, which are 5 times and 3 times lower than the Au\textendash Co reference coating, respectively.},
  langid = {english},
  keywords = {Au-Co coating,Electrodeposition,Tribology,Triton X-100,Tungsten disulfide}
}

