@article{nouailletas2015,
  title = {From the Conceptual Design to the First Simulation of the New {{WEST}} Plasma Control System},
  author = {Nouailletas, R. and Ravenel, N. and Signoret, J. and Treutterer, W. and Spring, A. and Lewerentz, M. and Rapson, C. J. and Masand, H. and Dhongde, J. and Moreau, P. and Guillerminet, B. and Br{\'e}mond, S. and Allegretti, L. and Raupp, G. and Werner, A. and Saint Laurent, F. and Nardon, E. and Bhandarkar, M.},
  year = {2015},
