}

@inproceedings{michizono2002,
  title = {Surface Charging by {{UV}} Irradiation at the Alumina {{RF}} Window},
  booktitle = {20th {{International Symposium}} on {{Discharges}} and {{Electrical Insulation}} in {{Vacuum}}},
  author = {Michizono, S. and Saito, Y.},
  year = {2002},
  month = jul,
  pages = {331--334},
  doi = {10/dxmhcj},
  abstract = {Alumina RF windows are used at high-power RF sources, such as klystrons. The breakdown of RF windows takes place due to multipactor and flashover events on the alumina surface. Both multipactor and surface flashover are considered to be related to the primary electron emission sites, such as the peripheral region of the alumina (the triple junction) and defects on the surface. Photoelectrons induced by ultraviolet light (UV) were emulated as primary electrons. The cathodoluminescence spectra during RF operation and the surface potential profile after the RF were measured and investigated.},
  keywords = {Al/sub 2/O/sub 3/,alumina,alumina RF windows,alumina surface,breakdown,cathodoluminescence,cathodoluminescence spectra,Ceramics,Coatings,Electric breakdown,Electron emission,flashover,Flashover,flashover events,klystrons,Klystrons,multipactor events,Optical surface waves,primary electron emission sites,Radio frequency,Resonance,surface charging,Surface charging,surface defects,triple junction}
}

@article{milanesio2007,
  title = {Analysis of the {{Tore Supra ICRF}} Antenna with {{TOPICA}}},
