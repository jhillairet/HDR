@article{vallejos2020,
  title = {Iterative Addition of Finite {{Larmor}} Radius Effects to Finite Element Models Using Wavelet Decomposition},
  author = {Vallejos, P. and Johnson, T. and Ragona, R. and Eester, D. Van and Zaar, B. and Hellsten, T.},
  year = {2020},
